
rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    
    // Allow public read access to all files
    match /{allPaths=**} {
      allow read;
    }

    // Allow authenticated users to write to team-photos folder
    match /team-photos/{allPaths=**} {
      allow write: if request.auth != null;
    }
  }
}
