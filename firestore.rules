
rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    // Allow public read/write access to all documents.
    // This is temporary to allow for initial database seeding.
    // WARNING: These rules are insecure and should be updated after seeding.
    match /{document=**} {
      allow read, write: if true;
    }
  }
}
